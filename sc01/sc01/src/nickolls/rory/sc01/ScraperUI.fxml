<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="203.0" prefWidth="265.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nickolls.rory.sc01.ScraperUIController">
   <children>
      <TitledPane prefHeight="107.0" prefWidth="265.0" text="Name Finder" VBox.vgrow="ALWAYS">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="70.0" prefWidth="263.0">
               <children>
                  <TextField fx:id="txtFld_email" layoutX="14.0" layoutY="14.0" promptText="email" />
                  <Button fx:id="btn_findName" layoutX="171.0" layoutY="14.0" mnemonicParsing="false" text="Find Name" />
                  <Label fx:id="lbl_name" alignment="CENTER" layoutX="15.0" layoutY="45.0" prefHeight="27.0" prefWidth="231.0" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
      <TitledPane prefHeight="132.0" prefWidth="265.0" text="URL Finder" VBox.vgrow="ALWAYS">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="txtFld_name" layoutX="14.0" layoutY="8.0" promptText="name" />
                  <Button fx:id="btn_findUrl" layoutX="175.0" layoutY="8.0" mnemonicParsing="false" text="Find URL" />
                  <TextField fx:id="txtFld_url" blendMode="MULTIPLY" editable="false" layoutX="14.0" layoutY="44.0" prefHeight="25.0" prefWidth="226.0" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</VBox>
